<Window x:Class="ParcelLockers.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ParcelLockers"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="1275"
        Closing="ProgramExit">
    <Canvas x:Name="MainCanvas">

        <Image x:Name="background" Source="/Resources/background.png" ></Image>
        
        <Image x:Name="p1" Source="/Resources/parcelLocker.png" Width="450"></Image>
        <Image x:Name="p2" Source="/Resources/parcelLocker.png" Width="450" Canvas.Left="400" ></Image>
        <Image x:Name="p3" Source="/Resources/parcelLocker.png" Width="450" Canvas.Left="800" ></Image>

        <Image x:Name="car" Source="/Resources/car.png" Width="450" Canvas.Left="10" Canvas.Top="576" ></Image>

    </Canvas>
</Window>
